{
  "usesTypeScript": true,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "None",
  "component": {
    "default": {
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true,
      "withIndex": true
    },
    "c": {
      "customTemplates": {
        "component": "src/templates/component/TemplateName.tsx",
        "style": "src/templates/component/TemplateName.module.scss",
        "index": "src/templates/component/index.ts"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withIndex": true
    },
    "lp": {
      "customTemplates": {
        "component": "src/templates/lazyPage/TemplateNamePage.tsx",
        "style": "src/templates/lazyPage/TemplateName.module.scss",
        "lazy": "src/templates/lazyPage/TemplateNamePage.lazy.tsx",
        "index": "src/templates/lazyPage/index.ts"
      },
      "path": "src/pages",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": true,
      "withIndex": true
    },
    "p": {
      "customTemplates": {
        "component": "src/templates/page/TemplateNamePage.tsx",
        "style": "src/templates/page/TemplateName.module.scss",
        "index": "src/templates/page/index.ts"
      },
      "path": "src/pages",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withIndex": true
    }
  }
}
